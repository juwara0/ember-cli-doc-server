{
  "name": "ember-cli-doc-server",
  "version": "1.1.0",
  "description": "An express middleware to add '/docs' url in application served by 'ember server' to load contents from 'docs' folder.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "homepage": "https://github.com/softlayer/ember-cli-doc-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/softlayer/ember-cli-doc-server.git"
  },
  "bugs": "https://github.com/softlayer/ember-cli-doc-server/issues",
  "engines": {
    "node": ">= 0.10.0"
  },
  "keywords": [
    "ember-addon",
    "documentation",
    "docs",
    "express",
    "server"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "serve-files-middleware",
      "history-support-middleware",
      "proxy-server-middleware"
    ]
  },
  "author": "Jeremy Brown <jeremy@notmessenger.com> (http://www.notmessenger.com)",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.0"
  },
  "devDependencies": {}
}
